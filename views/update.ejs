<div class="row mt-5">
    <div class="col-md-6 m-auto">
      <div class="card card-body">
        <h1 class="text-center mb-3"><i class="fas fa-sign-in-alt"></i> Update </h1>
        <% include ./partials/messages %>
        <form action="/admin/update" method="POST">
          <div class="form-group">
            <label for="laundryid">Laundry Id</label>
            <input
              type="number"
              id="laundryid"
              name="laundryid"
              class="form-control"
              placeholder="Enter User Laundry Id"
              required
            />
          </div>
          <div class="form-group">
            <input
              type="checkbox"
              id="stepone"
              name="stepone"
              class="form-control" 
            > Laundry Bag Given </input>
          </div>
          <div class="form-group">
            <input
              type="checkbox"
              id="steptwo"
              name="steptwo"
              class="form-control"
            />
            <label for="steptwo"> Laundry bag ready</label>
          </div>
          <div class="form-group">
            <label for="password">Password</label>
            <input
              type="password"
              id="password"
              name="password"
              class="form-control"
              placeholder="Enter password"
              required
            />
          </div>


          <button type="submit" class="btn btn-primary btn-block">Submit</button>
        </form>
      </div>
    </div>
  </div>